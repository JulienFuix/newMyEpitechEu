{
  "manifest_version": 2,
 
  "name": "my.epitech.eu with pourcents and style",
  "version": "1.0",
  "description": "Beautiful new epitech.eu on Chrome. Thanks to A.Hooks!!",
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [{
    "css": ["style.css"],
    "js": ["script.js"],
    "matches": ["https://my.epitech.eu/*"]
  }],
  "permissions": [
    "tabs"
  ],
  "icons": { 
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png", 
    "256": "256x256.png"
  }
}